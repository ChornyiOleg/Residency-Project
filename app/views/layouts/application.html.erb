<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Residency</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "all.min", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "animate", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "animated-text", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "application", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "bootstrap.min", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "flaticon", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "meanmenu.min", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "nivo-slider", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "owl.carousel.min", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "owl.transitions", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "responsive", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "settings", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "slick", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "slick-theme", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "style", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "theme-default", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "widget", media: 'all', "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "venobox.css", media: 'all', "data-turbo-track": "reload" %>
    <%= javascript_include_tag "application", "data-turbo-track": "reload", defer: true %>

    <%= javascript_include_tag "owl.carousel.min", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "slick.min", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "venobox.js", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "vendor/modernizr-3.5.0.min", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "ajax-mail", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "jquery.barfiller", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "jquery.counterup.min", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "jquery.meanmenu", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "jquery.nivo.slider.pack", "data-turbo-track": "reload", defer: true %>
    <%= javascript_include_tag "jquery.scrollUp", "data-turbo-track": "reload", defer: true %>

  </head>

  <body>
  <div class="content">
    <!--top-header-->
    <%= render 'include/home/top_header' %>
    <!--top-header-->

    <!--notifications-->
    <%= render 'include/home/notifications' %>
    <!--notifications-->

    <!--bottom-header-->
    <%= render 'include/home/bottom_header' %>
    <!--bottom-header-->

    <div>
      <%= yield %>
    </div>

    <!--footer-->
    <%= render 'include/home/footer' %>
    <!--footer-->

  </div>

  </body>
</html>
